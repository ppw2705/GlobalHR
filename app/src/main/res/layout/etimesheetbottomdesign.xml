<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout android:layout_height="wrap_content"
    android:layout_width="match_parent"
    android:layout_marginTop="10dp"
    android:layout_marginLeft="5dp"
    android:layout_marginRight="5dp"
    android:background="#d3d3d3"
    xmlns:android="http://schemas.android.com/apk/res/android">

    <LinearLayout
        android:id="@+id/lltable"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="horizontal"
        android:background="#ffffff"
        android:paddingTop="5dp"
        xmlns:android="http://schemas.android.com/apk/res/android"
        >
        <LinearLayout
            android:id="@+id/detailleft"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:orientation="vertical"
            android:paddingStart="15dp"
            android:paddingEnd="5dp"
            android:paddingBottom="10dp"
            >
            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:orientation="horizontal"
                android:layout_marginEnd="10dp"
                >
                <TextView
                    android:id="@+id/tsitemid"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="Ts item ID"
                    android:layout_marginStart="1dp"
                    android:layout_weight="1"
                    android:textColor="#000000"
                    android:layout_marginEnd="20dp"
                    android:textAlignment="viewStart"
                    android:textSize="10sp"
                    />
                <TextView
                    android:id="@+id/tsid"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="Ts ID"
                    android:textColor="#000000"
                    android:textAlignment="viewStart"
                    android:layout_weight="1"
                    android:textSize="10sp"
                    />

            </LinearLayout>
            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:orientation="horizontal"
                >
                <TextView
                    android:id="@+id/four"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="44"
                    android:background="#d3d3d3"
                    android:layout_weight="1"
                    android:textSize="13sp"
                    android:textColor="#000000"
                    android:textAlignment="center"
                    android:layout_margin="2dp"
                    />
                <TextView
                    android:id="@+id/tsid44"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="44"
                    android:textColor="#000000"
                    android:textAlignment="center"
                    android:background="#d3d3d3"
                    android:layout_weight="1"
                    android:layout_margin="2dp"
                    android:textSize="13sp"
                    />

            </LinearLayout>

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:orientation="vertical"
                >

                <TextView
                    android:id="@+id/tsdate"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:text="Ts Date"
                    android:layout_weight="1"
                    android:textSize="13sp"
                    android:textColor="#000000"
                    android:layout_margin="2dp"
                    />
                <TextView
                    android:id="@+id/dateints"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:text="2015-05-15"
                    android:paddingStart="10dp"
                    android:paddingEnd="10dp"
                    android:textColor="#000000"
                    android:textAlignment="center"
                    android:background="#d3d3d3"
                    android:layout_weight="1"
                    android:layout_margin="2dp"
                    android:textSize="13sp"
                    />

            </LinearLayout>

        </LinearLayout>
        <View
            android:id="@+id/view"
            android:layout_height="55dp"
            android:layout_width="1dp"
            android:layout_alignRight="@id/tsid44"

            android:layout_marginTop="10dp"
            android:layout_marginRight="-10dp"
            android:background="#a0a0a0"/>

        <RelativeLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:layout_marginLeft="15dp"
            >

            <TextView
                android:id="@+id/textView"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:textColor="#a0a0a0"
                android:textSize="16sp"
                android:layout_alignParentStart="true"
                android:layout_alignParentTop="true"
                android:layout_marginTop="1dp"
                android:text="Ts Project" />
            <TextView
                android:id="@+id/textView2"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:textColor="#000000"
                android:textSize="12sp"
                android:textStyle="bold"
                android:layout_below="@+id/textView"
                android:text="Global HR Created via Timesheet" />
            <TextView
                android:id="@+id/textView3"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:textColor="#a0a0a0"
                android:textSize="16sp"
                android:layout_below="@+id/textView2"
                android:text="Ts Activity" />
            <TextView
                android:id="@+id/textView4"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:textColor="#000000"
                android:textSize="12sp"
                android:layout_below="@+id/textView3"
                android:textStyle="bold"
                android:text="Global HR Created via Timesheet" />
            <ImageView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:src="@drawable/deletewhite"
                android:background="#fe5a01"
                android:layout_alignTop="@+id/textView"
                android:layout_alignParentEnd="true"
                android:id="@+id/imageView5" />
            <ImageView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:src="@drawable/editwhite"
                android:layout_marginTop="2dp"
                android:background="#44c101"
                android:layout_below="@+id/imageView5"
                android:layout_alignEnd="@+id/imageView5" />

        </RelativeLayout>
